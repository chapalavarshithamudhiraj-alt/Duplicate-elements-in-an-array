#include <stdio.h>
int main() {
    int n, i, j, flag;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter elements: ");
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    printf("Duplicate elements: ");
    for(i = 0; i < n; i++) {
        flag = 0;
        for(j = 0; j < i; j++) {
            if(arr[i] == arr[j]) {
                flag = 1;
                break;
            }
        }
        if(flag == 0) {
            for(j = i+1; j < n; j++) {
                if(arr[i] == arr[j]) {
                    printf("%d ", arr[i]);
                    break;
                }
            }
        }
    }
    printf("\n");
    return 0;
}
